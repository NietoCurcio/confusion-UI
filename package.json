{
  "name": "confusion",
  "version": "1.0.0",
  "description": "This is a website for Ristorante Con Fusion",
  "main": "index.html",
  "scripts": {
    "start": "npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "sass": "sass --watch src/sass:src/css",
    "dev": "concurrently \"npm run sass\" \"npm run lite\"",
    "clean": "rimraf dist",
    "imagemin": "imagemin src/img/* -o dist/img",
    "minifyhtml": "html-minifier -c config-html-minifier.json --input-dir ./src --output-dir dist --file-ext html",
    "minifycss": "node build-clean-css.js",
    "minifyjs": "node build-terser.js",
    "build": "npm run clean & npm run imagemin & npm run minifyhtml & npm run minifycss & npm run minifyjs"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.27.4",
    "clean-css": "^5.1.3",
    "concurrently": "^6.2.0",
    "del": "^6.0.0",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^4.0.0",
    "grunt-contrib-htmlmin": "^3.1.0",
    "grunt-contrib-imagemin": "^4.0.0",
    "grunt-contrib-uglify": "^5.0.1",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-filerev": "^2.3.1",
    "grunt-sass": "^3.1.0",
    "grunt-usemin": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^5.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-usemin": "^0.3.30",
    "jit-grunt": "^0.10.0",
    "lite-server": "^2.3.0",
    "load-grunt-tasks": "^5.1.0",
    "node-sass": "^6.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.35.1",
    "time-grunt": "^2.0.0"
  },
  "dependencies": {
    "bootstrap": "^5.0.1",
    "grunt": "^1.4.1",
    "terser": "^5.7.1"
  }
}
